{
  "ProcessingSettings": {
    "GeneratorRunAsProcess": false,
    "GenerateFile": false,
    "FilePath": "C:\\14.02.2026_BillEvent.csv",
    "ThreadCount": 3,
    "ProcessType": "Task"
  },
  "GeneratorSettings": {
    "FileFormat": "csv",
    "PaidBillEventCount": 1000000,
    "CancelledBillEventCount": 2000000
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=Collector;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  }
}