{
  "ProcessingSettings": {
    "RunAsProcess": false,
    "GenerateFile": false,
    "FilePath": "C:\\23.12.2025_BillEvent.csv",
    "ThreadCount": 2,
    "ProcessType": "Thread"
  },
  "GeneratorSettings": {
    "FileFormat": "csv",
    "PaidBillEventCount": 700000,
    "CancelledBillEventCount": 300000
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=Collector;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  }
}